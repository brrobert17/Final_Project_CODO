{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "types": [
      "react-native",
      "react-native-reanimated"
    ],
    "paths": {
      "@components/*": [
        "./components/*"
      ],
      "@interfaces": [
        "./utils/interfaces.ts"
      ],
      "@screens/*": [
        "./screens/*"
      ],
      "@assets/*": [
        "./assets/*"
      ],
      "@gStyle": [
        "./style.ts"
      ],
      "@dbConn/*": [
        "./DatabaseConn/*"
      ],
      "@utils/*": [
        "./utils/*"
      ],
    }
  }
}